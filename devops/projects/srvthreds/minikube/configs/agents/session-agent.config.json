{
  "name": "Manages user sessions and participant interactions",
  "description": "Manages user sessions and participant interactions",
  "command": {
    "entrypoint": "node",
    "args": [
      "dist-server/agent/agent.js",
      "-c",
      "session_agent",
      "-i",
      "org.wt.session1",
      "-d"
    ]
  },
  "ports": {
    "http": 3000,
    "websocket": 3001
  },
  "resources": {
    "memory": {
      "request": "128Mi",
      "limit": "256Mi"
    },
    "cpu": {
      "request": "100m",
      "limit": "300m"
    }
  }
}
